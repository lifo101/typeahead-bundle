{
    "name": "lifo/typeahead-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle that provides a Bootstrap Typeahead form type.",
    "keywords": ["symfony","typeahead","autocomplete"],
    "homepage": "https://github.com/lifo101/symfony-typeahead-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Morriss",
            "email": "lifo2013@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.1"
    },
    "suggest":  {
        "twitter/bootstrap": "2.3.*",
        "mopa/bootstrap-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Lifo\\TypeaheadBundle": "" }
    },
    "target-dir": "Lifo/TypeaheadBundle",
    "extra": {
        "branch-alias": {
        }
    }
}
